}

.nav {
  position: fixed;
  left: 0;
  width: 100%;
  height: 44px;
  line-height: 44px;
  text-align: center;
  background: #fff;
  z-index: 999;
  color: #333333;
}

.nav-back {
  position: absolute;
  left: 8rpx;
  font-size: 44rpx;
  font-weight: bold;
  color: #000;
}

.nav .title {
  font-size: 34rpx;
  font-weight: bold;
}
