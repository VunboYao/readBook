.custom-navbar {
  background: #fff;
}

.nav {
  position: fixed;
  left: 0;
  width: 100%;
  height: 44px;
  line-height: 44px;
  text-align: center;
  background: #fff;
  z-index: 999;
  color: #333333;
}

.filling-div {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  z-index: 999;
}

.icon-wrap {
  position: absolute;
  left: 4px;
  top: 2px;
  font-size: 44rpx;
  font-weight: bold;
  color: #000;
}

.nav .title {
  font-size: 34rpx;
  font-weight: bold;
}
